<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.np.mad.freeflow">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Freeflow">
        <activity
            android:name=".TaskActivity"
            android:exported="false" />
        <activity
            android:name=".NewCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".NewTaskActivity"
            android:exported="false" />
        <activity
            android:name=".JoinWorkspaceActivity"
            android:exported="false" />
        <activity
            android:name=".WorkspaceInviteActivity"
            android:exported="false" />
        <activity
            android:name=".WorkspaceSettingsActivity"
            android:exported="false" />
        <activity
            android:name=".WorkspaceActivity"
            android:exported="false" />
        <activity
            android:name=".NewWorkspaceActivity"
            android:exported="false" />
        <activity
            android:name=".AccountActivity"
            android:exported="false" />
        <activity
            android:name=".SignInActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>